<div class="container">
<h1 class="text-center mt-4">Edit Category</h1>

<% if @category.errors.any? %>
    <h2>The following errors prevented the category from being saved</h2>
    <ul>
      <% @category.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  <% end %>

  <%= render 'form'%>
  
  
</div>